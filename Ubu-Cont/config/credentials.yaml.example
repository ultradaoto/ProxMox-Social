# Credentials Configuration
# COPY THIS FILE TO credentials.yaml AND FILL IN VALUES
# DO NOT COMMIT credentials.yaml TO VERSION CONTROL!

# VNC Server Password
vnc:
  password: "your_vnc_password_here"

# Accounts (for automation tasks)
# These are used when the agent needs to log into services
accounts:
  google:
    email: "your.email@gmail.com"
    password: "your_password"
    # Or use app-specific password
    app_password: ""

  microsoft:
    email: ""
    password: ""

  # Add other accounts as needed
  custom_service:
    url: "https://example.com/login"
    username: ""
    password: ""

# API Keys (if needed for external services)
api_keys:
  openai: ""
  anthropic: ""

# Proxy Configuration (if needed)
proxy:
  enabled: false
  http: ""
  https: ""
  no_proxy:
    - "localhost"
    - "192.168.100.0/24"
